	<string>OpenFlight requires access to the photo library to save media.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>OpenFlight requires access to the photo library to save media.</string>
	<key>UIAppFonts</key>
	<array>
		<string>Rajdhani-Bold.ttf</string>
		<string>Rajdhani-Light.ttf</string>
		<string>Rajdhani-Medium.ttf</string>
		<string>Rajdhani-Regular.ttf</string>
		<string>Rajdhani-SemiBold.ttf</string>
	</array>
